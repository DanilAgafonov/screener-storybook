{
  "name": "screener-storybook",
  "version": "0.6.0",
  "description": "Automated Visual Testing for React Storybook using Screener.io",
  "repository": {
    "type": "git",
    "url": "https://github.com/screener-io/screener-storybook.git"
  },
  "bin": {
    "screener-storybook": "./bin/screener-storybook.js"
  },
  "scripts": {
    "test": "npm run lint && nyc mocha \"test/**/*.spec.js\" && npm run coverage",
    "lint": "eslint .",
    "coverage": "nyc check-coverage --statements 90 --functions 90 --branches 80 --lines 90"
  },
  "nyc": {
    "include": [
      "src/index.js",
      "src/runner.js",
      "src/validate.js"
    ]
  },
  "engines": {
    "node": ">= 4"
  },
  "license": "MIT",
  "peerDependencies": {
    "@kadira/storybook": "^2.17.0",
    "react": "^0.14.7 || ^15.0.0"
  },
  "dependencies": {
    "bluebird": "~3.4.6",
    "colors": "~1.1.2",
    "commander": "~2.9.0",
    "joi": "~10.0.2",
    "jsdom": "~9.8.3",
    "lodash": "~4.17.2",
    "portfinder": "~1.0.10",
    "react": "^15.0.0",
    "react-dom": "~15.4.1",
    "request": "~2.81.0",
    "screener-runner": "^0.6.0",
    "semver": "~5.3.0"
  },
  "devDependencies": {
    "chai": "~3.5.0",
    "eslint": "~3.11.1",
    "mocha": "~3.1.2",
    "nyc": "~8.4.0",
    "rewire": "~2.5.2"
  }
}
