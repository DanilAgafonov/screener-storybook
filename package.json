{
  "name": "screener-storybook",
  "version": "0.9.9",
  "description": "Automated Visual Testing for React Storybook using Screener.io",
  "repository": {
    "type": "git",
    "url": "https://github.com/screener-io/screener-storybook.git"
  },
  "bin": {
    "screener-storybook": "./bin/screener-storybook.js"
  },
  "scripts": {
    "test": "npm run lint && nyc mocha \"test/**/*.spec.js\" && npm run coverage",
    "lint": "eslint .",
    "coverage": "nyc check-coverage --statements 90 --functions 90 --branches 80 --lines 90"
  },
  "nyc": {
    "include": [
      "src/check.js",
      "src/index.js",
      "src/runner.js",
      "src/validate.js"
    ]
  },
  "engines": {
    "node": ">= 4"
  },
  "license": "MIT",
  "dependencies": {
    "bluebird": "~3.4.6",
    "colors": "~1.1.2",
    "commander": "~2.9.0",
    "get-port": "~3.2.0",
    "joi": "~10.0.2",
    "jsdom": "~9.8.3",
    "lodash": "~4.17.2",
    "prop-types": "^15.6.0",
    "react": "^15.0.0",
    "react-dom": "~15.4.1",
    "request": "~2.81.0",
    "requestretry": "~1.12.2",
    "screener-runner": "^0.7.7"
  },
  "devDependencies": {
    "chai": "~3.5.0",
    "eslint": "~3.11.1",
    "mocha": "~3.1.2",
    "nyc": "~8.4.0",
    "rewire": "~2.5.2"
  }
}
